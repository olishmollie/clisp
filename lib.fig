(def newline "\n")


(def null? (lambda (x)
    (eq? x '())))


(def and (lambda (x y)
    (cond (x
           (cond (y true)
                 (true false)))
          (true false))))

(def or (lambda (x y)
    (cond (x true)
          (true (cond (y true)
                      (true false))))))

(def append (lambda (x y)
    (cond ((null? x) y)
           (true (cons (car x) (append (cdr x) y))))))
