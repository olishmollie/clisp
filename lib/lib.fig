(define lib "lib/lib.fig")

;; lists
(define (list . args) args)

(define (cadr x)
  (car (cdr x)))
(define (caddr x)
  (car (cdr (cdr x))))

(define (null? x)
  (if (atom x) (eq x nil) #f))

(define (not x)
  (if x #f #t))

(define (pair? x) (not (atom x)))

(define (fact n)
  (define (fact-iter prod count max)
    (if (> count max)
        prod
        (fact-iter (* prod count) (+ count 1) max)))
  (fact-iter 1 1 n))
